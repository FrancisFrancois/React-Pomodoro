{
  "name": "react-pomodoro",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "dev": "parcel src/index.html --public-url /",
    "build": "parcel build src/index.html --public-url ./",
    "prepare": "husky install",
    "start": "npm run watch:css && react-scripts start",
    "buiild": "npm run watch:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch:css": "postcss src/styles/tailwind.css -o src/styles/output.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FrancisFrancois/React-Pomodoro.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FrancisFrancois/React-Pomodoro/issues"
  },
  "homepage": "https://github.com/FrancisFrancois/React-Pomodoro#readme",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@becode/eslint-config": "^0.12.1",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-unicorn": "^38.0.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.0.2",
    "parcel": "^2.0.1",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.2",
    "prettier": "^2.4.1",
    "tailwindcss": "^2.2.19"
  },
  "dependencies": {
    "moment": "^2.29.1",
    "moment-duration-format": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "lint-staged": {
    "*.json": [
      "npx prettier --write"
    ],
    "*.js": [
      "npx eslint --fix --cache"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      "@babel/preset-env"
    ]
  }
}
